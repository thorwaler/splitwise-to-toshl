{
  "name": "splitwise-to-toshl",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node_modules/.bin/concurrently \"yarn dev:client\" \"yarn dev:server\"",
    "dev:client": "vite --host 0.0.0.0 --port 5554",
    "dev:server": "node server.js",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "docker:build": "yarn build && docker build -t splitwise-to-toshl .",
    "docker:buildx": "yarn build && docker buildx build --platform linux/amd64 -t splitwise-to-toshl . --load",
    "docker:run": "docker run -p 2567:2567 splitwise-to-toshl",
    "docker:save": "docker save -o splitwise-to-toshl.tar splitwise-to-toshl"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.19",
    "@mui/material": "^5.15.19",
    "date-fns": "^3.6.0",
    "express": "^4.19.2",
    "finalhandler": "^1.2.0",
    "http": "^0.0.1-security",
    "http-proxy-middleware": "^3.0.0",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "serve-static": "^1.15.0",
    "url": "^0.11.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  }
}
